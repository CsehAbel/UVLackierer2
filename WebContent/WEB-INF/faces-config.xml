<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>nincsrecept</from-outcome>
			<to-view-id>NincsRecept.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>vanrecept</from-outcome>
			<to-view-id>LejCikk.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>LejCikk.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>nincslejcikk</from-outcome>
			<to-view-id>NincsLejCikk.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>vanlejcikk</from-outcome>
			<to-view-id>Sorrend.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
